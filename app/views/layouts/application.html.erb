<!DOCTYPE html>
<html>
<head>
  <title>OrangeWalrus</title>
  <%= favicon_link_tag %>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
<% if !flash[:notice].blank? %>
  <div class="notice">
    <%= flash[:notice] %>
  </div>
 <% end %>
<div id="wrap">
	<%= render(:partial => 'shared/nav') %>
	<div id="main">
      
      <div id="content">
        <%= yield %>
      </div>
    </div>
</div>
</body>
</html>
